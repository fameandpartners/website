<% if @personalization.persisted? %>
  productPersonalizationPopup.success(
    '<%=j render('product_personalizations/thanks') %>',
    '<%=j render('spree/products/customization_popup') %>',
    <%= @personalization.variant_id %>,
    '<%=raw j( { order: CartSerializer.new(current_order).to_json }.to_json )%>'
  );
<% else %>
  productPersonalizationPopup.fail('<%=j render('product_personalizations/form', personalization: @personalization) %>');
<% end %>
