<div class="section-row columns">
  <div class="column is-8-desktop is-offset-2-desktop ">
    <div class="page-content content-collapse-expand">
      <div id="panel-articles" class="tab-pane active" role='tabpanel'>
        <div class="panel-group" roll='tablist' aria-multiselectable='true'>
          <% contents[:collapsible_entries].each do |item| %>
            <%= render partial: "layouts/contentful/modules/items/ITEM--collapsible-entry", locals: {contents: item} %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
