        $('.js-trigger-generator').on('click', function() {
          var result;
          var user_measurements = {};
          var units             = 'inches';
          var bust              = $('#bust').val() || 0;
          var underbust         = $('#underbust').val() || 0;
          var waist             = $('#waist').val() || 0;
          var hips              = $('#hips').val() || 0;

          user_measurements.bust_circumference       = bust;
          user_measurements.under_bust_circumference = underbust;
          user_measurements.waist_circumference      = waist;
          user_measurements.hip_circumference        = hips;

          if ($('#switch-to-cm').is(':checked')) {
            units = 'cm';
          } else {
            user_measurements.bust_circumference       = convert_to_cm(bust);
            user_measurements.under_bust_circumference = convert_to_cm(underbust);
            user_measurements.waist_circumference      = convert_to_cm(waist);
            user_measurements.hip_circumference        = convert_to_cm(hips);
          }

          result = calculate_body_match(user_measurements);

          if (result.shape.name.length > 0) {
            $('.js-result-title').html('Your Body Type and Dress Size is');
            $('.js-result-type').html(result.shape.name);
            $('.js-result-size').html(result.size.presentation);
            $('.js-result-description').html(result.shape.description);
          } else {
            $('.js-result-title').html('');
            $('.js-result-type').html('');
            $('.js-result-size').html('');
            $('.js-result-description')
              .html("Sorry, we don't have a match for you right now. Try the Standard fit sample in the size the customer usually wears and go from there.");
          }

          $('.js-result-bust').html(bust + ' ' + units);
          $('.js-result-underbust').html(underbust + ' ' + units);
          $('.js-result-waist').html(waist + ' ' + units);
          $('.js-result-hips').html(hips + ' ' + units);

